{
  "name": "qfield",
  "version-string": "dev",
  "port-version": 1,
  "homepage": "https://github.com/opengisch/qfield",
  "description": "A simplified and touch optimized frontend for QGIS.",
  "dependencies": [
    {
      "name": "gdal",
      "default-features": false
    },
    "qgis-qt6",
    {
      "name": "qtsensors",
      "features": [
        "qml"
      ]
    },
    "qtimageformats",
    "qtwebview",
    "qtconnectivity",
    "qtdeclarative",
    "qtlocation",
    {
      "name": "qtcharts",
      "features": [
        "qml"
      ]
    },
    {
      "name": "qtmultimedia",
      "features": [
        "qml",
        {
          "name": "ffmpeg",
          "platform": "linux | android"
        }
      ]
    },
    "qtserialport",
    {
      "name": "qca-qt6",
      "default-features": false
    },
    "nu-book-zxing-cpp",
    {
      "name": "sentry-native",
      "platform": "!android & !ios & !osx"
    },
    "spix-qt6",
    "catch2",
    {
      "name": "sentry-cocoa",
      "platform": "ios | osx"
    }
  ]
}
